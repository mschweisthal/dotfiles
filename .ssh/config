Host *
    Compression=yes
    ControlMaster auto
    ControlPath /tmp/%r@%h:%p
    ControlPersist yes
    ForwardAgent no
    ForwardX11 no
    ForwardX11Trusted yes
    Protocol 2
    ServerAliveInterval=15
    ServerAliveCountMax=6
    TCPKeepAlive=yes

Host github.com
    Hostname ssh.github.com
    Port 443
